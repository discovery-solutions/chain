{
    "name": "@discovery-solutions/chain",
    "version": "0.0.1",
    "type": "module",
    "description": "AI orchestration for product builders. Build, manage, and optimize AI-powered workflows with ease.",
    "author": "Lucas Paes <lucas@dscvr.space>",
    "scripts": {
        "dev": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json --watch",
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "lint": "eslint . --ext .ts,.tsx",
        "test": "vitest",
        "test:examples": "node examples/test-all.js"
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/esm/index.d.ts"
        },
        "./client": {
            "import": "./dist/esm/client/index.js",
            "require": "./dist/cjs/client/index.js",
            "types": "./dist/esm/client/index.d.ts"
        }
    },
    "peerDependencies": {
        "ai": "^5.0.76"
    },
    "devDependencies": {
        "@ai-sdk/openai": "^2.0.53",
        "@types/node": "^20",
        "dotenv": "^17.2.3",
        "eslint": "^9",
        "prettier": "^3.0.0",
        "tsup": "^8.5.0",
        "typescript": "^5",
        "vitest": "^3.2.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/discovery-solutions/struct.git"
    },
    "bugs": {
        "url": "https://github.com/discovery-solutions/struct/issues"
    },
    "license": "MIT",
    "keywords": [],
    "homepage": "https://github.com/discovery-solutions/struct#readme",
    "files": [
        "dist",
        "src"
    ]
}
